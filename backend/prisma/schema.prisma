generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String    @id @default(dbgenerated())
  password String
  name     String
  email    String    @unique
  phone    String    @unique
  // quizzes  QuizData[]
}

model lab {
  id       String    @id @default(dbgenerated())
  lab_name   String
  owner_name String
  phone    String    @unique
  email    String    @unique
  password String
  license_no String   @unique
  gst_no     String    @unique
  address    String
  city       String
  state      String
  pincode    String
  // tests     Tests[]
}

model Tests {
  id       String    @id @default(dbgenerated())
  test_name String
  test_price String
  test_description String
  lab_id    String
  // lab       lab      @relation(fields: [lab_id], references: [id])

}